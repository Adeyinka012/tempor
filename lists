my_list =[1,2,3,4,5,6,7,8,9,0]
# length of list
print(len(my_list))
print(my_list[4])
print(my_list[-1])
# slicing in list
print(my_list[-1])
# my_list[4] = 10
# print(my_list)
my_list.append(25)
print(my_list)
# How many times certain element occurred
my_list.count(9)
print(my_list.count(9))
# remove element from the list
# print(my_list.pop())

